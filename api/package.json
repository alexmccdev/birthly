{
    "name": "api",
    "version": "0.0.1",
    "description": "Birthly API",
    "main": "index.js",
    "scripts": {
        "dev:start": "ts-node-dev --watch .env.development -r dotenv/config --respawn src/index.ts dotenv_config_path=./.env.development",
        "dev:generate": "dotenv -e .env.development -- npx prisma generate && npm run dev:copy-prisma-types",
        "dev:migrate": "dotenv -e .env.development -- npx prisma migrate dev && npm run dev:copy-prisma-types",
        "dev:studio": "dotenv -e .env.development -- npx prisma studio",
        "dev:seed": "dotenv -e .env.development -- npx prisma db seed --preview-feature",
        "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
        "dev:copy-prisma-types": "cd ./prisma/ && ts-node postTypeGeneration.ts",
        "dev:test": "dotenv -e .env.development -- mocha -r ts-node/register src/**/*.test.ts"
    },
    "author": "Squidbody",
    "license": "ISC",
    "_moduleAliases": {
        "@controllers": "src/controllers",
        "@routes": "src/routes",
        "@utils": "src/utils",
        "@middleware": "src/middleware"
    },
    "dependencies": {
        "@prisma/client": "^3.2.1",
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-rate-limit": "^5.4.1",
        "http-errors": "^1.8.0",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.6.5"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/chai": "^4.2.22",
        "@types/chai-http": "^4.2.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-rate-limit": "^5.1.3",
        "@types/faker": "^5.5.8",
        "@types/http-errors": "^1.8.1",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.10.3",
        "@types/node-cache": "^4.2.5",
        "@types/nodemailer": "^6.4.4",
        "@types/supertest": "^2.0.11",
        "chai": "^4.3.4",
        "chai-http": "^4.3.0",
        "dotenv-cli": "^4.0.0",
        "faker": "^5.5.3",
        "mocha": "^9.1.2",
        "prisma": "^3.2.1",
        "supertest": "^6.1.6",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.3"
    }
}
