{
    "name": "api",
    "version": "0.0.1",
    "description": "Birthly API",
    "main": "index.js",
    "scripts": {
        "start:dev": "ts-node-dev -r dotenv/config --respawn src/index.ts dotenv_config_path=./.env.development",
        "generate:dev": "dotenv -e .env.development -- npx prisma generate",
        "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev",
        "studio:dev": "dotenv -e .env.development -- npx prisma studio"
    },
    "author": "Squidbody",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^2.29.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^16.6.1",
        "cross-env": "^7.0.3",
        "prisma": "^2.29.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    }
}
