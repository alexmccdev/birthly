{
    "name": "api",
    "version": "0.0.1",
    "description": "Birthly API",
    "main": "index.js",
    "scripts": {
        "dev:start": "ts-node-dev --watch .env.development -r dotenv/config --respawn src/index.ts dotenv_config_path=./.env.development",
        "dev:generate": "dotenv -e .env.development -- npx prisma generate",
        "dev:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
        "dev:studio": "dotenv -e .env.development -- npx prisma studio",
        "dev:seed": "dotenv -e .env.development -- npx prisma db seed --preview-feature",
        "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\""
    },
    "author": "Squidbody",
    "license": "ISC",
    "_moduleAliases": {
        "@controllers": "src/controllers",
        "@routes": "src/routes",
        "@utils": "src/utils",
        "@middleware": "src/middleware"
    },
    "dependencies": {
        "@prisma/client": "^2.29.1",
        "bcrypt": "^5.0.1",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "http-errors": "^1.8.0",
        "jsonwebtoken": "^8.5.1",
        "module-alias": "^2.2.2",
        "node-cache": "^5.1.2"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express": "^4.17.13",
        "@types/faker": "^5.5.8",
        "@types/http-errors": "^1.8.1",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "^16.6.1",
        "@types/node-cache": "^4.2.5",
        "dotenv-cli": "^4.0.0",
        "faker": "^5.5.3",
        "prisma": "^2.29.1",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    }
}
